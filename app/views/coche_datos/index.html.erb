<p id="notice"><%= notice %></p>

<h1>Coche Datos</h1>

<p>
  Press <%= link_to 'here', search_coche_datos_path %> to search for coche datos.
</p>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @coche_datos.each do |coche| %>
      <tr>
				<td><%= coche.car %></td>
				<td><%= coche.mpg %></td>
				<td><%= coche.cylinders %></td>
				<td><%= coche.displacement %></td>
				<td><%= coche.horsepower %></td>
				<td><%= coche.weight %></td>
				<td><%= coche.acceleration %></td>
				<td><%= coche.model %></td>
				<td><%= coche.origin %></td>
        <td><%= link_to 'Show', coche %></td>
        <td><%= link_to 'Edit', edit_coche_dato_path(coche) %></td>
        <td><%= link_to 'Destroy', coche, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Coche Dato', new_coche_dato_path %>
